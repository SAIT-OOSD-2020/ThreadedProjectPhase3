<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.PackagesController">
    <children>
      <VBox prefHeight="568.0" prefWidth="1000.0">
         <children>
              <HBox prefHeight="568.0" prefWidth="1000.0">
                  <children>
                        <TableView fx:id="tblPackages" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="600.0" prefWidth="940.0">
                            <columns>
                                <TableColumn fx:id="colPkgName" prefWidth="120.0" text="Name" />
                                <TableColumn fx:id="colPkgStartDate" text="Start Date" />
                                <TableColumn fx:id="colPkgEndDate" text="End Date" />
                                <TableColumn fx:id="colPkgDesc" prefWidth="160.0" text="Description" />
                                <TableColumn fx:id="colPkgBasePrice" text="Base Price" />
                                <TableColumn fx:id="colPkgAgencyCommission" text="Commission" />
                            </columns>
                        </TableView>
                  </children>
              </HBox>
                  <HBox alignment="CENTER" prefHeight="0.0" prefWidth="940.0" spacing="100.0">
                      <children>
                          <Button fx:id="btnPackageAdd" alignment="CENTER" maxHeight="40.0" maxWidth="100.0" minHeight="40.0" minWidth="100.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" style="-fx-background-radius: 8px;" text="Add" textAlignment="CENTER" />
                          <Button fx:id="btnPackageEdit" alignment="CENTER" maxHeight="40.0" maxWidth="100.0" minHeight="40.0" minWidth="100.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" style="-fx-background-radius: 8px;" text="Edit" textAlignment="CENTER" />
                          <Button fx:id="btnPackageDelete" alignment="CENTER" maxHeight="40.0" maxWidth="100.0" minHeight="40.0" minWidth="100.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" style="-fx-background-radius: 8px;" text="Delete" textAlignment="CENTER" />
                      </children>
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
                  </HBox>
         </children>
         <padding>
            <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
         </padding>
      </VBox>
    </children>
</AnchorPane>
